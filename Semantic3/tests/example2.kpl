Program Example2; (* Factorial *)

Var n : Integer;

Function G(n : Integer) : Integer;
  Begin
    If n = 0 Then G := 1 Else G := N * G (N - 1);
  End;

Function F(n : Integer) : Integer;
VAR X : T;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
    G := F(G);
  End;

Begin
